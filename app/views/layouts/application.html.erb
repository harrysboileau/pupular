<!DOCTYPE html>
<html>
<head>
  <title>Pupular</title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>

  <% if current_dog && current_dog.is_registered? %>
    <%= render "layouts/top_navigation" %>
    <%= render "layouts/search" %>
  




  <% end %>
  <div class="container">
    <%= yield %>
  </div>
  <% if current_dog && current_dog.is_registered? %>
    <%= render "layouts/bottom_navigation" %>
  <% end %>


</body>
</html>
